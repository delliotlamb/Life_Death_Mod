            <Target>Player</Target>
        </Prereq>			
        <Prereq>
            <Type>Tech</Type>
            <Attribute>Ereogs_Journal</Attribute>
        </Prereq>
        <LaborToBuild>376</LaborToBuild>
        <GameModifier>
            <ModType>Resource</ModType>
